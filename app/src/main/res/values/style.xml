<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="numberPickerCustomSize" parent = "Theme.AppCompat.Light">
        <item name="android:textSize">10sp</item>
        <item name="android:editTextColor">@color/black</item>
        <item name="android:textColorPrimary">@color/black</item>
    </style>
    <style name="Widget.MaterialComponents.BottomSheet.DragHandle" parent="Widget.Material3.BottomSheet.DragHandle">
        <item name="tint">?colorOnSurface</item>
        <!--Alpha can be added optionally to achieve nice color blend-->
        <item name="android:alpha">0.3</item>
    </style>
    <style name="CustomChipChoice"
        parent="@style/Widget.MaterialComponents.Chip.Filter">
        <item name ="chipBackgroundColor">@color/chip_background_state_list</item>
        <item name ="android:textColor">@color/chip_text_state_list</item>
    </style>

</resources>